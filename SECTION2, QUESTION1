package digi;

public class Digi {

    //Write a for loop that prints all the multiples of 3 and 5 up to 100: 3, 5, 6, 9, 10...100

    public static void main(String[] args) {
        
        for (int i=0; i<100; i++)
        {
            if (i%3 == 0 || i%5 == 0)
            {
                System.out.println(i);
            }
        }
    }
}
